                    <Button Content="Read" Click="OnReadPairing_AD_Key" />

                    <Button Content="Notify" Click="OnNotifyPairing_AD_Key" />

                </StackPanel>


            </StackPanel>
        </controls:Expander>

        <controls:Expander Header="BR_Utilities" IsExpanded="false" Style="{StaticResource HeaderStyleExpander}">
